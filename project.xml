<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="OneHourOpenLife" package="com.project.OneHourOpenLife" version="1.0.0" company="" />
	<app main="Main" path="bin" file="OneHourOpenLife" />
	<window background="0" width="1280" height="720" fps="60" vsync="true" hardware="true" depth-buffer="false" stencil-buffer="false" />
	<source path="src" />
	<haxelib name="format"/>
	<haxelib name="openfl" />
	<haxelib name="actuate"/>
	<haxelib name="svg"/>
	<haxelib name="hscript" unless="html5"/>
	<haxelib name="discord_rpc" if="cpp"/>

	<!--<icon path="icon.svg" />-->
	<icon path="icon.png" width="1024" height="1024" />
	<haxedef name="dom" if="html5"/>
</project>